[
  {
    "kernelSource": {
      "name": "pixel4",
      "repo": "https://github.com/johnatahs23/kernel_google_msm-4.14",
      "branch": "13",
      "device": "flame",
      "defconfig": "floral_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "name": "clang",
        "binaryEnv": [
          "bin"
        ]
      },
      {
        "repo": "https://github.com/GrowtopiaJaw/aarch64-linux-android-4.9.git",
        "branch": "google",
        "name": "aarch64",
        "binaryEnv": [
          "bin"
        ]
      },
      {
        "repo": "https://github.com/KudProject/arm-linux-androideabi-4.9",
        "branch": "master",
        "name": "arm",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": "y"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": true
  }
]
